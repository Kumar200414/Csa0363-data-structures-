#include<stdio.h>
int factorial(int n){
	if (n==0||n==1){
		return 1;
	}else{
		return n*factorial(n-1);
	}
}
int main() {
	int num;
	printf("enter a number :");
	scanf("%d",&num);
	printf("factorial of %d is %d\n",num,factorial(num));
	return 0;
}
output:
Enter the number5
factorial of number120:
--------------------------------
Process exited after 18.59 seconds with return value 0
